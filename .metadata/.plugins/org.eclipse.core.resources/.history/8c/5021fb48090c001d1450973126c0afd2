package com.globallogic.helloworld;
import java.util.*;
public class compex {
public static void main(String[] args) {
	ArrayList<Integer> ar=new ArrayList<Integer>();
	ar.add(10);
	ar.add(20);
	ar.add(3);
	ar.add(5);
	Collections.sort(ar);
	System.out.println(ar);
	Collections.shuffle(ar);
	System.out.println(ar);
}
}